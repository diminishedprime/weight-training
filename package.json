{
  "name": "weight-training",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:gen-types": "supabase gen types typescript --schema 'public' --local > src/database.types.ts",
    "db:new-migration": "supabase migration new",
    "db:new-test": "supabase test new",
    "db:check-unused": "scripts/check-for-unused-public-rpcs",
    "db:reset": "pnpm run db:start && supabase db reset && pnpm run db:gen-types && pnpm run format && pnpm run db:check-unused",
    "db:start": "supabase start",
    "db:test": "pnpm run db:start && supabase test db",
    "dev": "next dev --turbopack",
    "format": "pnpm run format:ui && pnpm run format:sql",
    "format:ui": "prettier --write '**/*.{js,ts,tsx,json,md}'",
    "format:sql": "find supabase -name '*.sql' -print | xargs -L 1 -P 16 -t pnpx sql-formatter --fix -l postgresql",
    "lint": "next lint",
    "start": "next start",
    "test": "dotenv -e .env.local -- pnpm run db:start && vitest --run",
    "test:coverage": "dotenv -e .env.local -- pnpm run db:start && vitest run --coverage",
    "test:mutation": "dotenv -e .env.local -- pnpm run db:start && stryker run",
    "test:ui": "dotenv -e .env.local -- pnpm run db:start && vitest --ui",
    "test:watch": "dotenv -e .env.local -- pnpm run db:start && vitest",
    "prepare": "husky"
  },
  "dependencies": {
    "@auth/supabase-adapter": "^1.9.1",
    "@date-io/date-fns": "^3.2.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/material-nextjs": "^7.1.1",
    "@mui/x-date-pickers": "^8.5.1",
    "@supabase/supabase-js": "^2.49.8",
    "d3-scale": "^4.0.2",
    "date-fns": "^4.1.0",
    "fuse.js": "^7.1.0",
    "immutable": "^5.1.3",
    "lodash": "^4.17.21",
    "next": "15.3.1",
    "next-auth": "5.0.0-beta.27",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "use-debounce": "^10.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@stryker-mutator/core": "^9.0.1",
    "@stryker-mutator/typescript-checker": "^9.0.1",
    "@stryker-mutator/vitest-runner": "^9.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3-scale": "^4.0.9",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "c8": "^10.1.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "happy-dom": "^17.6.3",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "stryker-cli": "^1.0.2",
    "supabase": "^2.30.4",
    "typescript": "^5",
    "vite": "^7.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  }
}

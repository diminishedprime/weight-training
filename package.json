{
  "name": "gatsby-starter-ts",
  "description": "A TypeScript starter for Gatsby that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <hey@joaopedro.dev> (https://joaopedro.dev)",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "test": "jest",
    "commit": "cz",
    "firebase": "firebase",
    "deploy": "gatsby build && firebase deploy --only hosting"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.6.0",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.1.1",
    "@mui/material": "5.1.1",
    "firebase": "9.5.0",
    "gatsby": "4.1.4",
    "gatsby-plugin-emotion": "7.2.0",
    "gatsby-plugin-google-gtag": "4.3.0",
    "gatsby-plugin-material-ui": "4.0.2",
    "gatsby-plugin-use-query-params": "1.0.1",
    "lodash": "4.17.21",
    "moment": "2.29.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-spinners": "0.11.0",
    "serialize-query-params": "1.3.5",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "use-query-params": "1.2.3"
  },
  "devDependencies": {
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@testing-library/dom": "8.11.1",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.177",
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "babel-jest": "27.3.1",
    "babel-preset-gatsby": "2.1.2",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "firebase-tools": "9.23.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.3.1",
    "lint-staged": "12.0.2",
    "prettier": "2.4.1",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}

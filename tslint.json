{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [true, "never-prefix"],
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "trailing-comma": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"]
  },
  "rulesDirectory": []
}
